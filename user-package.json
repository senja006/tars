{
    "dependencies": {
        "jquery": "^3.1.0",
        "normalize.css": "^4.2.0",
        "es6-promise": "^3.2.1",
        "handlebars-helpers": "^0.10.0"
    },
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/preset-env": "^7.4.5",
        "postcss-flexbugs-fixes": "^2.0.0",
        "vinyl-ftp": "^0.5.0"
    },
  "scripts": {
    "tarsInit": "tars init --silent --source https://github.com/senja006/tars/archive/senja006.zip",
    "deployFrontDev": "tars start deploy --flags '--dev --frontend'",
    "deployFrontDevWatch": "tars dev --custom-flags '--deploywatcher --dev --frontend'",
    "deployFrontBuild": "tars build --min && tars start deploy --flags '--build --frontend'",
    "deployBackDev": "tars start deploy --flags '--dev --backend'",
    "deployBackDevWatch": "tars dev --custom-flags '--deploywatcher --dev --backend'",
    "deployBackBuild": "tars build --min && tars start deploy --flags '--build --backend'",
    "deployLocalDev": "tars dev --custom-flags '--deploywatcher --dev --local'",
    "deployLocalDevHtml": "tars dev --custom-flags '--deploywatcher --dev --local --html'",
    "deployLocalBuild": "tars build --min && tars start deploy --flags '--build --local'",
    "deployLocalBuildHtml": "tars build --min && tars start deploy --flags '--build --local --html'",
    "deployServer": "tars start deploy --flags '--server'",
    "deployServerBuildHtml": "npm run deployLocalBuildHtml && tars start deploy --flags '--server'",
    "updateProject": "tars update-project --source https://github.com/senja006/tars/archive/senja006.zip",
    "backstopInit": "backstop init",
    "backstopApprove": "backstop approve",
    "backstopTest": "backstop test",
    "backstopTestRm": "rm -rf backstop_data/bitmaps_test"
  }
}
